      "dashboards": [
        {
          "lookmlID": "sales_analytics::sales_leadership_quarter_overview",
          "name": "Sales & Marketing Leadership",
          "permalink": "sales_leadership_overview"
        }
          ],
        "defaultDashboard": "sales_leadership_overview"
        },
    "filters": [
      {
        "autoSuggest": {
        "model": "sales_analytics",
        "view": "account"
