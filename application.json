{
  "exploreConfig": {
    "data": true,
    "filters": true,
    "sidebar": true,
    "visualization": true
  },
  "theme": {
    "colors": {
      "primary": "#6AA9EF",
      "primaryDark": "#6297D1",
      "primaryDarker": "#4E7DB0"
    }
  },
  "name": "Sales Analytics",
  "tables": [{
    "id": "sales_rep",
    "name": "Sales Rep",
    "model": "sales_analytics",
    "view": "opportunity",
    "params": "f[opportunity.close_date]=1+years&sorts=opportunity.total_closed_won_amount+desc&limit=1000",
    "keyColumn": "opportunity_owner.name",
    "columns": [{
        "name": "Sales Rep",
        "field": "opportunity_owner.name"
      },
      {
        "name": "Total Revenue",
        "field": "opportunity.total_closed_won_amount"
      },
      {
        "name": "New Business Won",
        "field": "opportunity.count_new_business_won"
      },
      {
        "name": "Opportunities Won",
        "field": "opportunity.count_won"
      }
    ]
  }],
  "home": {
    "name": "Sales Leadership Overview",
    "permalink": "sales_leadership_overview",
    "type": "dashboardCollection",
    "dashboardCollection": {
      "dashboards": [{
        "lookmlID": "sales_analytics::sales_leadership_quarter_overview",
        "name": "Sales & Marketing Leadership",
        "permalink": "sales_leadership_overview"
      }],
      "defaultDashboard": "sales_leadership_overview"
    },
    "filters": [{
      "autoSuggest": {
        "model": "sales_analytics",
        "view": "account"
      },
      "field": "account.billing_state",
      "name": "State",
      "type": "Select",
      "dashboardAlias": "State"
    }]
  },
  "sections": [{
