          },
          "filters": [{
              "field": "opportunity_owner.name",
              "dashboardAlias": "Sales Rep",
              "name": "Sales Rep",
              "type": "Table",
              "defaultValue": "Kevin Heller",
              "tableID": "sales_rep"
            }
          ]
        },
        {
          "name": "Operations",
          "icon": "web-analytics/acquisitions",
